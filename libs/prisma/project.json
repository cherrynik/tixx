{
  "name": "prisma",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/prisma/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/prisma",
        "main": "libs/prisma/src/index.ts",
        "tsConfig": "libs/prisma/tsconfig.lib.json",
        "assets": ["libs/prisma/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/prisma/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/libs/prisma"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/libs/prisma"
      }
    },
    "migrate-dev": {
      "command": "npx prisma migrate dev",
      "options": {
        "cwd": "libs/prisma"
      }
    },
    "db-push": {
      "command": "npx prisma db push",
      "options": {
        "cwd": "libs/prisma"
      }
    },
    "gen": {
      "command": "npx prisma generate",
      "options": {
        "cwd": "libs/prisma"
      }
    }
  },

  "tags": []
}
